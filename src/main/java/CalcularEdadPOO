public class CalcularEdad {
    
    public static void main (String args[]){
        Scanner sc = new Scanner (System.in);
        
        int ano ,anoactual ;
        int Resultado;
        System.out.println("Dame tu fecha de nacimiento");
        ano = sc.nextInt();
        System.out.println("Dame el a√±o actual prro");
        anoactual = sc.nextInt();
        
        Resultado = ano - anoactual;
        
        System.out.println("Tu edad es:"+Resultado);
        
        
        
    }
    
}
